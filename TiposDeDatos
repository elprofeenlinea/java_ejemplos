package Variables;

/*Programa que permite indentificar los distintos tipos de datos
  Usados en java
  Elaborado por: MiscursosEnlinea
*/

public class TiposDeDatos {

    int edad;
    double estatura;
    String nombre, apellidos;
    boolean estadoActivo;
    
    public void inicializarDatos() {
        edad = 20;
        estatura = 1.72;
        nombre = "Juan";
        apellidos =  "Montes";
        estadoActivo = true;
    }
    
    
    public static void main(String[] args) {
        
    }
    
}
